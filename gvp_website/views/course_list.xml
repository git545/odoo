<odoo>
      <record id="action_course_list" model="ir.actions.act_window">
        <field name="name">Course List</field>
        <field name="res_model">tbl_course</field>
    </record>
    <record id="course_name_search" model="ir.ui.view">
        <field name="name">CourseName: search</field>
        <field name="model">tbl_course</field>
        <field name="arch" type="xml">
            <search>
                <field name="cname"/>
            </search>
        </field>
</record>
   <record id="gvp_course_list" model="ir.ui.view">
        <field name="name">Course: list</field>
        <field name="model">tbl_course</field>
        <field name="arch" type="xml">
            <tree>
                <field name="cname"/>
           </tree>
    </field>
</record>
</odoo>