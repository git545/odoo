<odoo>
      <record id="year_list_action" model="ir.actions.act_window">
        <field name="name">Admission/pass out year List</field>
        <field name="res_model">tbl_year</field>
    </record>
    <record id="year_name_search" model="ir.ui.view">
        <field name="name">Admission/pass out year: search</field>
        <field name="model">tbl_year</field>
        <field name="arch" type="xml">
            <search>
                <field name="start_year"/>
                <field name="pass_year"/>
            </search>
        </field>
</record>
   <record id="gvp_year_list" model="ir.ui.view">
        <field name="name">Admission/pass out year: list</field>
        <field name="model">tbl_year</field>
        <field name="arch" type="xml">
            <tree>
                <field name="start_year"/>
                <field name="pass_year"/>
           </tree>
    </field>
</record>
</odoo>